  },
  "publisher": "Tripletex",
  "author": "Tripletex AS",
  "license": "MIT",
  "devDependencies": {
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-fetch": "^2.6.1",
    "np": "^7.5.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "xlsx": "^0.17.0"
  },
  "dependencies": {
    "moment": "^2.27.0"
  },
  "np": {
    "yarn": false,
    "tests": false
  }
