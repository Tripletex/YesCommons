{
  "name": "yescommons",
  "version": "1.0.0",
  "description": "Common tools needed for developing and testing accounting systems in Norway",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prestart": "npm run download-bankregistry",
    "start": "webpack-dev-server --open",
    "watch": "webpack --watch",
    "build": "webpack",
    "download-bankregistry": "curl -o bank.xlsx https://www.bits.no/document/iban/ && node bankregisterdownloader.js",
    "deploy": "np",
    "postdeploy": "bs --no-yarn --hook build"
  },
  "keywords": [
    "accounting",
    "erp"
  ],
  "author": "Tripletex AS",
  "license": "MIT",
  "devDependencies": {
    "branchsite": "^4.0.3",
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-fetch": "^2.6.1",
    "np": "^7.5.0",
    "webpack": "^5.44.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "xlsx": "^0.17.0"
  },
  "dependencies": {
    "moment": "^2.27.0"
  },
  "np": {
    "yarn": false,
    "contents": "build"
  }
}
