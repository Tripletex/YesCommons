{
  "name": "@tripletex/yescommons",
  "version": "1.6.0",
  "description": "Common tools needed for developing and testing accounting systems in Norway",
  "scripts": {
    "test": "env TS_NODE_PROJECT=\"tsconfig.test.json\" mocha --require ts-node/register 'src/test/**/*.ts'",
    "prestart": "npm run download-bankregistry",
    "start": "webpack-dev-server --open",
    "watch": "webpack --watch",
    "build": "webpack",
    "download-bankregistry": "curl -o bank.xlsx https://www.bits.no/document/iban/ && node bankregisterdownloader.js",
    "deploy": "np",
    "start:dev": "webpack-dev-server --mode development --hot --open",
    "prettier": "prettier --write \"**/*.ts\""
  },
  "keywords": [
    "accounting",
    "erp",
    "economics",
    "tools",
    "kid",
    "account number",
    "mod11",
    "mod10"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Tripletex/YesCommons"
  },
  "publishConfig": {
    "access": "public"
  },
  "publisher": "Tripletex",
  "author": "Tripletex AS",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "html-webpack-plugin": "^5.3.2",
    "mocha": "^9.0.3",
    "moment-locales-webpack-plugin": "^1.2.0",
    "node-fetch": "^2.6.1",
    "np": "^7.5.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5",
    "webpack": "^5.44.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2",
    "xlsx": "^0.17.0"
  },
  "dependencies": {
    "moment": "^2.27.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.2"
  },
  "np": {
    "yarn": false,
    "tests": false
  }
}
