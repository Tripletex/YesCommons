    "moment-locales-webpack-plugin": "^1.2.0",
    "node-fetch": "^2.6.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5",
    "webpack": "^5.38.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.2",
    "xlsx": "^0.17.0"
  },
  "dependencies": {
    "moment": "^2.27.0",
    "moment-locales-webpack-plugin": "^1.2.0",
    "source-map-loader": "^3.0.0",
    "ts-loader": "^9.2.2"
